package Test_constructor;

class Test {
	    int a; 
	    int b; 
	    static void fun1(int a,int b) 
	    { 
	       // a = 20;                             //Line 1
	        b = a*10;                             //Line 2
	        fun2();                             //Line 3
	        System.out.print(b);           //Line 4
	    } 
	    static void fun2() 
	    {     
	        System.out.println("from m2"); 
	    }
}
	

